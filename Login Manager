#include <iostream>
#include <fstream>
#include <stdlib.h>

using namespace std;

int main() {
  ifstream in("login.txt");
  string line;
  getline(in, line);
  in.close();
  string command = "ssh pi@IP_ADDRESS; raspberry; sudo python3 KEYBOARD_EMULATOR.py " + line;
  system(command.c_str());
  ExitWindows(0, 0);
}
